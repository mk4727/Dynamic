
     <%-include('common/head.ejs')-%>
      <title>Hello, world!</title>
   </head>
   <body>
    <%-include('common/header.ejs')-%>
      <div class="container-fluid bg-side-nav">
         <div class="row">
            <div class="col-lg-2 sidebaare">
                <%-include('common/sidebaar.ejs')-%>
               
            </div>
            <div class="col-lg-10">

               <br><br>

              <% if (typeof success !== 'undefined' && success !== '') { %>
               <div class="alert alert-success" role="alert">
                  <%= success %>
               </div>
            <% } else if (typeof err !== 'undefined' && err !== '') { %>
               <div class="alert alert-danger" role="alert">
                  <%= err %>
               </div>
            <% } %>




              <div class="dashboard-content-area">
                <!-- put your contente -->

               <h1>All Category</h1>
               <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                  <thead>
                     <tr>
                        <th>Category Photo</th>
                        <th>Category </th>
                        <th>Category Url</th>
                        <th>Category Title</th>
                        <th>Category Meta Keyword</th>
                        <th>Category Meta Desc</th>
                        <th>Category Details</th>
                        <th>Edit</th>
                        <th>Delete</th>
                     </tr>
                  </thead>
                  <tbody>
                     <%x.forEach((y)=>{%>

                    
                     <tr>  
                        <td>
                        
                           <%if(y.categoryPhoto){%>
                              <img src="/backend/images/<%=y.categoryPhoto%>" width="70">
                              <%}else{%>
                                 <img src="/backend/images/no-img.png" width="70">
                              <%}%>
                        </td>                      
                        <td><%=y.category%></td>
                        <td><%=y.categoryUrl%></td>

                        <td><%=y.categoryTitle%></td>
                        <td><%=y.categoryMeta%></td>
                        <td><%=y.categoryDesc%></td>

                        <td><%-y.categoryDetails.slice(0, 200)-%></td>
                        <td>
                           <a class="btn btn-primary" href="/admin/category/edit/<%=y.category%>">Edit</a> </td>
                           <td>                    
                           <form style="display: inline;" action="/admin/category/delete/<%=y.category%>?_method=DELETE" method="POST">
                              <button style="cursor: pointer;" type="submit" class="btn btn-danger ml-2">Delete</button>
                           </form>
                        </td>
                     </tr>
                     <%})%>
                     
                  </tbody>
               </table>

              <!-- end your content here -->
             </div>

            </div>
         </div>
      </div>

     <%-include('common/footer.ejs')-%>